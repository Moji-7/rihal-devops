{
  "projectType": "library",
  "sourceRoot": "libs/shared-widgets/src",
  "prefix": "rihal",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared-widgets"],
      "options": {
        "jestConfig": "libs/shared-widgets/jest.config.ts",
        "passWithNoTests": true
